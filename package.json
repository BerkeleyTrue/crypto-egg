{
  "name": "crypto-egg",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "sc": "shadow-cljs",
    "server": "shadow-cljs stop && shadow-cljs start",
    "start:cli": "shadow-cljs watch cli",
    "start:server": "shadow-cljs watch server",
    "dev:server": "node ./build/server.js",
    "dev:cli": "node ./build/cli.js",
    "start": "npm run start:server",
    "cards": "shadow-cljs watch cards",
    "build:cli": "shadow-cljs release cli && ncc build build/cli.js -o bin/ --target es2020 && mv -f bin/index.js bin/crypto-egg",
    "build:cli:install": "./scripts/install.sh",
    "build:server": "shadow-cljs release server",
    "build:docker": "docker build -f docker/Dockerfile -t crypto-egg/server .",
    "test": "shadow-cljs compile test && node out/test.js",
    "test:watch": "shadow-cljs watch test --config-merge \"{:autorun true}\"",
    "e2e": "shadow-cljs compile e2e && node out/e2e.js",
    "report": "shadow-cljs run shadow.cljs.build-report app report.html",
    "lint": "clj-kondo --lint src",
    "format": "zprint-clj --hang -i \"./src/**/*.{clj,cljs,cljc,edn}\" -o ./ && zprint-clj -i \"./*.edn\" -o ./"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.33.1",
    "clj-kondo": "^2022.1.15",
    "serve-handler": "~6.1.3",
    "shadow-cljs": "^2.16.12",
    "taiko": "~1.0.12",
    "taiko-diagnostics": "~0.4.0",
    "zprint-clj": "~0.8.0"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "backoff-rxjs": "^6.5.7",
    "chalk": "^4.1.2",
    "concat-stream": "1.6.2",
    "content-type": "1.0.4",
    "cookies": "0.8.0",
    "etag": "1.8.1",
    "eth-provider": "^0.9.7",
    "invariant": "^2.2.4",
    "lru": "3.1.0",
    "multiparty": "4.1.3",
    "qs": "6.5.1",
    "random-bytes": "1.0.0",
    "rxjs": "^7.5.2",
    "simple-encryptor": "1.2.0",
    "url": "0.11.0",
    "web3": "^1.6.0",
    "ws": "5.1.1",
    "xregexp": "^5.1.0",
    "yargs": "^17.2.1"
  }
}
